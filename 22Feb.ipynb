    "# extract number is a custom function to extract the view count\n",
    "def extract_number(string):\n",
    "    numbers = []\n",
    "    for char in string:\n",
    "        if char.isdigit():\n",
    "            numbers.append(char)\n",
    "        else:\n",
    "            continue\n",
    "    if numbers:\n",
    "        extracted_number = int(\"\".join(numbers))\n",
    "    return extracted_number\n",
    "\n",
    "# temporary list\n",
    "videos_data = []\n",
